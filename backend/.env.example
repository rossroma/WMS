# 服务器配置
PORT=3000
NODE_ENV=development
CORS_ORIGIN=http://localhost:3000

# 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_USER=wms_user
DB_PASSWORD=wms_password
DB_NAME=wms_db

# JWT配置
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=24h

# 日志配置
LOG_LEVEL=info
LOG_FILE=logs/app.log
LOG_MAX_SIZE=10m
LOG_MAX_FILES=7

# 文件上传配置
UPLOAD_MAX_SIZE=5242880
UPLOAD_PATH=uploads

# 七牛云配置
QINIU_ACCESS_KEY=your_access_key
QINIU_SECRET_KEY=your_secret_key
QINIU_BUCKET=your_bucket_name
QINIU_DOMAIN=your_domain
QINIU_ZONE=z0
QINIU_USE_HTTPS=true
QINIU_USE_CDN=false

# 定时任务配置
CRON_MESSAGE_CLEANUP=0 0 * * *
MESSAGE_RETENTION_DAYS=30
CRON_INVENTORY_CHECK=0 */4 * * *
CRON_RETRY_ATTEMPTS=3 